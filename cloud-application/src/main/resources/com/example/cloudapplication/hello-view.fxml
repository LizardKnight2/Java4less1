<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="434.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.cloudapplication.ChatController">
   <HBox prefHeight="26.0" prefWidth="600.0">
      <children>
         <Button mnemonicParsing="false" text="pathUpReq" />
      </children>
   </HBox>
      <HBox VBox.vgrow="ALWAYS">
            <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0">
                  <HBox prefHeight="100.0" prefWidth="200.0" />
                  <ListView fx:id="clientView" layoutX="-2.0" prefHeight="400.0" prefWidth="298.0" />
                  <ListView fx:id="serverView" layoutX="294.0" prefHeight="400.0" prefWidth="307.0" />
            </AnchorPane>
            

            <TableView VBox.vgrow="ALWAYS" />
            <TextField fx:id="pathField" editable="false" HBox.hgrow="ALWAYS" />
            
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0">
            <Button mnemonicParsing="false" onAction="#upload" text="Add file" />
            <Button mnemonicParsing="false" onAction="#download" text="remove file" />
      <Button mnemonicParsing="false" onAction="#delete" text="delete" />
      <!--<Button mnemonicParsing="false" onAction="#pathUpReq" text="PathUpReq" />-->
      <Button mnemonicParsing="false" onAction="#pathInReq" text="PathInReq" />
      </HBox>
</VBox>
